    <servlet-class>onlineSurveyPackage.LoginInfoFetch</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginInfoFetch</servlet-name>
    <url-pattern>/myself</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginInfoExtraction</display-name>
